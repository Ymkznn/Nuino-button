{
    "builds": [
        {
            "src": "generate.py", 
            "use": "@vercel/static-build" 
        },
        {
            "src": "api/main.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ]
}